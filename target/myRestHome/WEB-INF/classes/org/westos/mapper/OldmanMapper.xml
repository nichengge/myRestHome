	</delete>
	<!-- 查找没有房间住的老人 -->
	<select id="selectNoRoomOldman" resultMap="BaseResultMap">
		select * from oldman where id not in 
		(select oldman.id from room,oldman where room.oldid=oldman.id)
		<if test="userId!=null">
			and user_id = #{userId}
		</if>
	</select>








