	<!-- 查询行 -->
	<select id="selectAllLine" parameterType="org.westos.entity.Oldman"
		resultType="java.lang.Integer">
		select count(id)
		from oldman where 1=1
		<if test="oldman.name != null and oldman.name != ''">
			and name like '%' #{oldman.name} '%'
		</if>
		<if test="oldman.gender != null and oldman.gender != ''">
			and gender = #{oldman.gender}
		</if>
		<if test="oldman.age != null and oldman.age != ''">
			and age > #{oldman.age}
		</if>
		<if test="oldman.userId != null and oldman.userId != ''">
			and user_id = #{oldman.userId}
		</if>
	</select>
	<!-- 分页查询 -->
	<select id="selectUserByPage" resultMap="OldmanResultMap">
		select
		<include refid="userandold" />
		from oldman,user where 1=1 and oldman.user_id=user.id
		<if test="oldman.name != null and oldman.name != ''">
			and name like '%' #{oldman.name} '%'
		</if>
		<if test="oldman.gender != null and oldman.gender != ''">
